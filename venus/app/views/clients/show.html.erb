<p id="notice"><%= notice %></p>

<div class="panel panel-primary col-md-offset-2 col-md-3">
  <div class="panel-heading">Cliente</div>
    <div class="row">
      <p>
        <strong>Rut:</strong>
        <%= @client.rut %>
      </p>

      <p>
        <strong>Nombre:</strong>
        <%= @client.nombre %>
      </p>

      <p>
        <strong>Telefono:</strong>
        <%= @client.telefono %>
      </p>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-md-offset-3">
    <%= link_to 'Editar', edit_client_path(@client) %> |
    <%= link_to 'Atras', clients_path %>
  </div>
</div>